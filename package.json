{
  "name": "manush-ai-task",
  "version": "1.0.0",
  "description": "Manush AI recruitment assignment",
  "main": "server.js",
  "scripts": {
    "dev": ". ./loadEnv ./dev.env && nodemon server.js",
    "worker": ". ./loadEnv ./dev.env && node ./queue/worker.js",
    "test": ". ./loadEnv ./test.env && jest --watch --coverage --config ./jest.config.js",
    "migrate-dev": ". ./loadEnv ./dev.env && knex migrate:latest",
    "migrate-test": ". ./loadEnv ./test.env && knex migrate:latest",
    "seed-dev": ". ./loadEnv ./dev.env && knex seed:run",
    "seed-make": ". ./loadEnv ./dev.env && knex seed:make 01-users",
    "api-doc": "apidoc -i controllers/ -o apidoc/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/caffeines/manush-ai-task.git"
  },
  "keywords": [
    "server",
    "backend",
    "api"
  ],
  "author": "caffeines",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/caffeines/manush-ai-task/issues"
  },
  "homepage": "https://github.com/caffeines/manush-ai-task#readme",
  "dependencies": {
    "amqplib": "^0.6.0",
    "bcrypt": "^5.0.0",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "express-validator": "^6.6.1",
    "helmet": "^4.1.0",
    "ioredis": "^4.17.3",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.5",
    "mailgun-js": "^0.22.0",
    "morgan": "^1.10.0",
    "pg": "^8.3.2",
    "redis": "^3.0.2",
    "short-uuid": "^3.1.1",
    "uuid": "^8.3.0"
  },
  "apidoc": {
    "name": "Manush AI",
    "version": "0.1.0",
    "description": "Manush AI backend task REST API documentation"
  },
  "nodemonConfig": {
    "ignore": [
      "test/*",
      "apidoc/*"
    ]
  },
  "devDependencies": {
    "eslint": "^7.7.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "jest": "^26.4.2",
    "supertest": "^4.0.2"
  }
}
